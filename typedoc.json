{
  "entryPoints": ["src/index.ts"],
  "out": "docs",
  "readme": "README.md",
  "includeVersion": true,
  "excludePrivate": true,
  "excludeExternals": true,
  "includeDeclarations": true,
  "plugin": ["typedoc-plugin-markdown"],
  "disableSources": false,
  "categorizeByGroup": true,
  "searchInComments": true,
  "validation": {
    "notExported": false,
    "invalidLink": true,
    "notDocumented": false
  }
}
